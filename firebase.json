{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source": "**",
      "headers": [ {
        "key": "Cross-Origin-Opener-Policy",
        "value": "same-origin"
      }, {
        "key": "Cross-Origin-Embedder-Policy",
        "value": "require-corp"
      } ]
    } ]
  }
}
